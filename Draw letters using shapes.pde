void setup()
{
  size(500,300);
  background(50,200,80);
}

void draw()
{
  noStroke();
  fill(255,255,0);
  ///draw N
  rect(50,50,20,100);
  quad(70,50,50,50,130,150,150,150);
  rect(130,50,20,100);
  ///draw X
  quad(220,50,200,50,280,150,300,150);
  quad(300,50,280,50,200,150,220,150);
  ///draw H
  rect(350,50,20,100);
  rect(430,50,20,100);
  rect(350,90,80,20);
  ///draw dot
  fill(255,200,0);
  ellipse(175,145,15,15);
  ellipse(325,145,15,15);
}
